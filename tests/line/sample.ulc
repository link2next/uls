#@ulc-2.4
#
#  ULC: Unified Lexical Configuration
#
DOMAIN: uls.link2next.io://season-1

ID_FIRST_CHARS: _ a-z A-Z
ID_CHARS: _ 0-9 a-z A-Z

NUMBER_PREFIXES: 0x:16 0:8

COMMENT_TYPE: /* */
COMMENT_TYPE: // \n

QUOTE_TYPE: token=DQUOTE "
QUOTE_TYPE: token=SQUOTE '

CASE_SENSITIVE: false

RENAME: NUMBER    NUM  
RENAME: LINENUM   LINENO

%%
#
# Token Definition List
# There are basically 3 columns per line, <token-name> <keyword> <token-id>.
# The third column can be omitted if you intend it to be the successor of the previous one.
#
LINK                -9
NONE                -8
TMPL                -7
LINENO              -6
ERR                 -5
EOF                 -4
EOI                 -3
ID                  -2
NUM                 -1

EQ       ==         128
LEQ      <=
GEQ      >=
NEQ      !=

AND      AND        140
AND      && 
OR       OR 
OR       ||
OR       or    

ASM      asm       150
DEFAULT  default
AUTO     auto
DO       do
DOUBLE   double

ELSE     else      160
ENUM     enum

GOTO     goto      165
IF       if
BREAK    break
CASE     case
SWITCH   switch
TYPEDEF  typedef   170
UNION    union
CHAR     char
EXTERN   extern
FLOAT    float
FOR      for       175
CONTINUE continue
CONST    const
INT      int
LONG     long
REGISTER register  180

RETURN   return    185
SHORT    short
SIGNED   signed
SIZEOF   sizeof
STATIC   static
STRUCT   struct    190
UNSIGNED unsigned
VOID     void
VOLATILE volatile
WHILE    while

LF       \n
TAB      \t
